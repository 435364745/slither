{
  "name": "slither",
  "version": "1.0.0",
  "description": "slither game",
  "main": "main.js",
  "scripts": {
    "build": "npm run lint && npm run release",
    "lint": "./node_modules/.bin/eslint ./src/**/*.js",
    "dev": "node server.js",
    "ws": "node socket.js",
    "dev:a": "concurrently \"npm run dev\" \"npm run ws\"",
    "release": "webpack --config ./webpack.dist.js --progress",
    "test": "./node_modules/.bin/mocha ./test/*.test.js --compilers js:babel-core/register --recursive --reporter spec"
  },
  "keywords": [
    "game",
    "slither"
  ],
  "dependencies": {
    "@types/node": "^6.0.45",
    "@types/ws": "0.0.34",
    "eventemitter3": "^2.0.2",
    "stats.js": "^0.16.0",
    "ws": "^1.1.1"
  },
  "author": "wanghx",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "concurrently": "^3.1.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "html-webpack-plugin": "^2.22.0",
    "mocha": "^3.1.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.14.1"
  }
}
